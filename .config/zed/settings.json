{
  "assistant": {
    "default_model": {
      "provider": "deepseek",
      "model": "deepseek-chat"
    },
    "version": "2"
  },
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "base_keymap": "VSCode",
  "ui_font_size": 16,
  "ui_font_family": "Geist Mono",
  "buffer_font_size": 16,
  "buffer_font_family": "Geist Mono",
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Atelier Cave Dark"
  },
  "languages": {
    "JavaScript": {
      "formatter": {
        "external": {
          "command": "prettier",
          "arguments": ["--stdin-filepath", "{buffer_path}"]
        }
      },
      "code_actions_on_format": {
        "source.fixAll.eslint": true
      }
    }
  }
}
